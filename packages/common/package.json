{
  "name": "@home-gallery/common",
  "version": "1.0.0",
  "description": "Common library of HomeGallery",
  "type":"module",
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "default": {
        "import": "./dist/node.js",
        "types": "./dist/node.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "npm-run-all -p build:js build:types",
    "build:js": "dev-cli build",
    "build:types": "tsc",
    "test": "tap",
    "watch": "dev-cli build --watch",
    "watch:test": "tap repl w"
  },
  "keywords": [
    "HomeGallery",
    "utils",
    "common",
    "lib"
  ],
  "author": "Sebastian Felis <sebastian@silef.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xemle/home-gallery.git",
    "directory": "packages/common"
  },
  "dependencies": {
    "@home-gallery/logger": "file:../logger",
    "@home-gallery/stream": "file:../stream",
    "ignore": "^7.0.5"
  },
  "devDependencies": {
    "@home-gallery/dev-tools": "file:../dev-tools"
  }
}
