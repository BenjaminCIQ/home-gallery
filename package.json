{
  "name": "filesystem-stats",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "cpx 'node_modules/@fortawesome/fontawesome-free/webfonts/*.*' src/webapp/fonts",
    "run": "node index.js",
    "dev": "node dev-server.js",
    "build": "parcel build src/webapp/index.html --out-dir dist/webapp --public-url ./",
    "//": "Styleguide",
    "styleguide:clean": "rimraf styleguide",
    "styleguide:build:kss": "kss --config kss-config.json",
    "styleguide:build:style": "sass -I node_modules src/webapp/app.scss styleguide/app.css",
    "styleguide:build": "npm-run-all styleguide:build:style -p styleguide:copy styleguide:build:kss",
    "styleguide:copy:assets": "cpx 'src/styleguide/*.{css,js}' styleguide",
    "styleguide:copy:fonts": "cpx 'src/webapp/fonts' styleguide",
    "styleguide:copy": "npm-run-all -p styleguide:copy:assets styleguide:copy:fonts",
    "styleguide:watch": "onchange 'src/webapp/**/*.scss' 'src/styleguide/**' -- npm run styleguide:build",
    "styleguide:serve": "live-server --port=3001 styleguide",
    "styleguide:dev": "npm-run-all styleguide:build -p styleguide:serve styleguide:watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "cpx": "^1.5.0",
    "debug": "^4.1.1",
    "http-proxy-middleware": "^0.20.0",
    "kss": "^3.0.0",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.2",
    "sass": "^1.23.7",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "easy-peasy": "^3.2.6",
    "exiftool-vendored": "^9.3.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^2.7.0",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "ignore": "^5.1.4",
    "memory-cache": "^0.2.0",
    "merge-stream": "^2.0.0",
    "moo": "^0.5.1",
    "morgan": "^1.9.1",
    "nearley": "^2.19.1",
    "node-vibrant": "^3.1.5",
    "react-router-dom": "^5.1.2",
    "react-router-last-location": "^2.0.1",
    "react-use-dimensions": "^1.2.1",
    "react-window": "^1.8.5",
    "request": "^2.88.0",
    "sharp": "^0.23.2",
    "sharp-phash": "^1.0.2",
    "split2": "^3.1.1",
    "through2": "^3.0.1",
    "use-in-view": "^1.0.15",
    "yargs": "^15.0.2"
  }
}
